    "x-logo" : {
      "url" : "http://www.allocatesoftware.com/wp-content/uploads/sites/31/2017/08/Allocate_logo_digital_Horizontal_RGB_retina.png",
      "backgroundColor" : "#FFFFc0",
      "altText" : "Allocate Software"
    }
  },
  "servers" : [ {
    "url" : "http://healthsuite.allocatesoftware.com/api/duty/v1"
  } ],
  "tags" : [ {
    "name" : "Duties"
  }, {
    "name" : "3rd Party Rostering"
  }, {
    "name" : "Costing"
  }, {
    "name" : "Bookings"
  } ],
  "paths" : {
    "/duties" : {
      "post" : {
        "description" : "Create a new Duty",
        "operationId" : "createDuty",
        "requestBody" : {
          "content" : {
            "application/json" : {
              "schema" : {
                "$ref" : "#/components/schemas/DutyDetails"
              }
            }
          },
          "description" : "Duty to be sent to agency",
          "required" : true
        },
        "responses" : {
          "200" : {
            "content" : {
              "text/plain" : {
                "schema" : {
                  "description" : "ID of the newly created duty",
                  "type" : "string"
                }
              }
            },
            "description" : "Duty created successfully"
          },
          "202" : {
            "content" : {
              "application/json" : {
                "schema" : {
                  "$ref" : "#/components/schemas/AsyncResponse"
                }
              }
            },
            "description" : "The duty has been accepted and a response will be returned asyncronously."
          }
        },
        "tags" : [ "Duties", "3rd Party Rostering" ]
      }
    },
    "/duties/{id}/costing" : {
      "post" : {
        "operationId" : "sendDutyCosting",
        "parameters" : [ {
          "description" : "ID of the duty",
          "explode" : false,
          "in" : "path",
          "name" : "id",
          "required" : true,
          "schema" : {
            "type" : "string"
          },
          "style" : "simple"
        } ],
        "requestBody" : {
          "content" : {
            "application/json" : {
              "schema" : {
                "$ref" : "#/components/schemas/DutyCostDetails"
              }
            }
          },
          "description" : "Costing Information",
          "required" : true
        },
        "responses" : {
          "200" : {
            "description" : "Duty costing successfully"
          },
          "202" : {
            "content" : {
              "application/json" : {
                "schema" : {
                  "$ref" : "#/components/schemas/AsyncResponse"
                }
              }
            },
            "description" : "The duty costing has been accepted and a response will be returned asyncronously."
          }
        },
        "tags" : [ "Duties", "Costing", "Bookings" ]
      }
    },
    "/duties/{id}/sendTo/{destination}" : {
      "post" : {
        "description" : "Send a duty for fulfillment by contingent workforce (Bank, Cloudstaff, Agency)",
        "operationId" : "sendDutyForFulfillment",
        "parameters" : [ {
          "description" : "ID of the duty",
          "explode" : false,
          "in" : "path",
          "name" : "id",
          "required" : true,
          "schema" : {
            "type" : "string"
          },
          "style" : "simple"
        }, {
          "description" : "Desination to send the duty",
          "explode" : false,
          "in" : "path",
          "name" : "destination",
          "required" : true,
          "schema" : {
            "enum" : [ "bank", "agency", "cloudstaff" ],
            "type" : "string"
          },
          "style" : "simple"
        } ],
        "responses" : {
          "200" : {
            "description" : "Duty sent successfully"
          },
          "202" : {
            "content" : {
              "application/json" : {
                "schema" : {
                  "$ref" : "#/components/schemas/AsyncResponse"
                }
              }
            },
            "description" : "Request been accepted and a response will be returned asyncronously."
          }
        },
        "tags" : [ "Duties", "3rd Party Rostering" ]
      }
    },
    "/duties/{id}" : {
      "delete" : {
        "description" : "Cancel a duty. Also cancels any bookings (if present).",
        "operationId" : "cancelDuty",
        "parameters" : [ {
          "description" : "ID of the duty",
          "explode" : false,
          "in" : "path",
          "name" : "id",
          "required" : true,
          "schema" : {
            "type" : "string"
          },
          "style" : "simple"
        } ],
        "responses" : {
          "200" : {
            "description" : "Duty cancelled"
          },
          "202" : {
            "content" : {
              "application/json" : {
                "schema" : {
                  "$ref" : "#/components/schemas/AsyncResponse"
                }
              }
            },
            "description" : "The cancellation has been accepted and a response will be returned asyncronously."
          }
        },
        "tags" : [ "Duties", "3rd Party Rostering" ]
      }
    },
    "/duties/{id}/booking" : {
      "delete" : {
        "description" : "Cancel a duty booking.",
        "operationId" : "cancelDutyBooking",
        "parameters" : [ {
          "description" : "ID of the duty",
          "explode" : false,
          "in" : "path",
          "name" : "id",
          "required" : true,
          "schema" : {
            "type" : "string"
          },
          "style" : "simple"
        } ],
        "responses" : {
          "200" : {
            "description" : "Duty booking cancelled"
          },
          "202" : {
            "content" : {
              "application/json" : {
                "schema" : {
                  "$ref" : "#/components/schemas/AsyncResponse"
                }
              }
            },
            "description" : "The duty cancellation been accepted and a response will be returned asyncronously."
          },
          "404" : {
            "description" : "Duty or Customer not found"
          }
        },
        "tags" : [ "Duties", "3rd Party Rostering" ]
      }
    }
  },
  "components" : {
    "schemas" : {
