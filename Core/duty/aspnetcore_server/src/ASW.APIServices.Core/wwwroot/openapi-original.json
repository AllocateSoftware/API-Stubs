        },
        "type" : "object"
      },
      "DutyDetails" : {
        "example" : {
          "requirements" : "{}",
          "where" : {
            "locationId" : "TRST.101"
          },
          "when" : {
            "from" : "2000-01-23T04:56:07.000+00:00",
            "to" : "2000-01-23T04:56:07.000+00:00"
          }
        },
        "properties" : {
          "when" : {
            "$ref" : "#/components/schemas/DutyDetails_when"
          },
          "where" : {
            "$ref" : "#/components/schemas/DutyDetails_where"
          },
          "requirements" : {
            "type" : "object"
          }
        },
        "type" : "object"
      },
      "DutyCostDetails" : {
        "example" : {
          "amount" : 0.8008281904610115,
          "costingType" : "estimated"
        },
        "properties" : {
          "costingType" : {
            "description" : "Whether this cost is estimated, or actual",
            "enum" : [ "estimated", "actual" ],
            "type" : "string"
          },
          "amount" : {
            "description" : "The amount (sterling)",
            "type" : "number"
          }
        },
        "type" : "object"
      },
      "EventResponse" : {
        "properties" : {
          "dutyId" : {
            "type" : "string"
          }
        }
      },
      "Event" : {
        "discriminator" : {
          "propertyName" : "eventType"
        },
        "properties" : {
          "eventType" : {
            "type" : "string"
          }
        },
        "required" : [ "eventType" ],
        "type" : "object"
      },
      "DutyEvent" : {
        "allOf" : [ {
          "$ref" : "#/components/schemas/Event"
        }, {
          "$ref" : "#/components/schemas/DutyEvent_allOf"
        } ]
      },
      "DateSpan" : {
        "properties" : {
          "start" : {
            "description" : "Start date-time",
            "format" : "date-time",
            "type" : "string"
          },
          "end" : {
            "description" : "End date-time",
            "format" : "date-time",
            "type" : "string"
          }
        },
        "type" : "object"
      },
      "DutyTimes" : {
        "properties" : {
          "period" : {
            "$ref" : "#/components/schemas/DutyTimes"
          },
          "restTime" : {
            "description" : "Number of minutes of rest date-time",
            "example" : 60,
            "type" : "integer"
          }
        },
        "type" : "object"
      },
      "Duty" : {
        "properties" : {
          "id" : {
            "description" : "Identifier for this duty",
            "example" : "ABCD-93370",
            "type" : "string"
          },
          "when" : {
            "$ref" : "#/components/schemas/Duty_when"
          },
          "where" : {
            "$ref" : "#/components/schemas/Duty_where"
          },
          "reason" : {
            "description" : "reason for requestBody",
            "type" : "string"
          },
          "requirements" : {
            "$ref" : "#/components/schemas/Duty_requirements"
          }
        },
        "type" : "object"
      },
      "DutySendEvent" : {
        "properties" : {
          "eventType" : {
            "type" : "string"
          },
          "destination" : {
            "description" : "Where this duty has been sent",
            "enum" : [ "bank", "agency", "cloudstaff" ],
            "type" : "string"
          },
          "duty" : {
            "$ref" : "#/components/schemas/Duty"
          }
        },
        "required" : [ "eventType" ],
        "type" : "object"
      },
      "DutyTempStaffingNoteCreatedEvent" : {
        "allOf" : [ {
          "$ref" : "#/components/schemas/Event"
