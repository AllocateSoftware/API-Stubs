            "type" : "string"
          },
          "to" : {
            "format" : "date-time",
            "type" : "string"
          }
        }
      },
      "DutyDetails_where" : {
        "example" : {
          "locationId" : "TRST.101"
        },
        "properties" : {
          "locationId" : {
            "example" : "TRST.101",
